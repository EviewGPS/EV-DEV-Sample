<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.sample.test"
    android:versionCode="5"
    android:versionName="1.5">

    <uses-permission android:name="android.permission.SHUTDOWN"/>
    <uses-permission android:name="android.permission.REBOOT"/>

    <!--for step count permission start-->
    <uses-permission android:name="com.eview.eCareShareDataProvider.read"/>
    <uses-permission android:name="com.eview.db.eCareProvider.readdb"/>
    <!--for step count permission end-->

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:name=".eCareApplication"
        android:theme="@style/AppTheme">
        <activity android:name="com.sample.test.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>